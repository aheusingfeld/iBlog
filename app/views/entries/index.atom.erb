<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">
    <title>i-Blog</title>
    <link rel="alternate" type="text/html" href="<%= entries_path %>" />
    <link rel="self" type="application/atom+xml" href="<%= entries_path( :atom ) %>" />
    <id>tag:internal.innoq.com/iblog,2009:/iblog//1</id>
    <updated><%= @entries[0].updated_at unless @entries.empty? %></updated>
    <subtitle>yet another internal innoQ blog</subtitle>

    <generator uri="https://internal.innoq.com/iblog/">i-Blogs Custom ATOM Feed Generator 1.0</generator>

<% @entries.each do |entry| %>
<entry>
    <title><%= h entry.title %></title>
    <link rel="alternate" type="text/html" href="<%= entry_path( entry )%>" />
    <id>tag:internal.innoq.com/iblog,2009:/iblog//1.<%= entry.id %></id>

    <published><%= entry.created_at %></published>

    <updated><%= entry.updated_at %></updated>

    <summary><%= textilize entry.body %></summary>
    <author>
        <name><%= h entry.author %></name>
        <uri>https://internal.innoq.com/iblog/</uri>
    </author>

    
    
    <content type="html" xml:lang="en" xml:base="https://internal.innoq.com/iblog/">
        <![CDATA[<%= textilize entry.body %>]]>
    </content>
</entry>
<% end %>
</feed>
